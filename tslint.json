{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "jsRules": {},
  "rules": {
    "no-consecutive-blank-lines": false,
    "semicolon": false,
    "align": false,
    "max-line-length": false,
    "variable-name": false,
    "no-unused-variable": [true, { "ignore-pattern": "jsx" }],
    "import-name": false
  },
  "rulesDirectory": []
}
